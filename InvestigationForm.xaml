        <DatePicker x:Name="dateTimePicker1" HorizontalAlignment="Left" Margin="12,37,0,0" VerticalAlignment="Top" Width="390"/>
        <DatePicker x:Name="dateTimePicker2" HorizontalAlignment="Left" Margin="12,87,0,0" VerticalAlignment="Top" Width="390"/>
